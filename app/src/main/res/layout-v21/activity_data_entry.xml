<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.ibxcodecat.frc_scouting.Activity.DataEntryActivity"
    tools:layout_editor_absoluteX="83dp"
    tools:layout_editor_absoluteY="-118dp">

    <TextView
        android:id="@+id/midAutoNumText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/_0_buckets"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/midAutoText" />

    <TextView
        android:id="@+id/lowAutoNumText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/_0_buckets"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/lowAutoText" />

    <Button
        android:id="@+id/cubeIncreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/Inc_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintStart_toEndOf="@+id/cubeNumText"
        app:layout_constraintTop_toTopOf="@+id/cubeNumText"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/hiAutoIncreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/Inc_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintStart_toEndOf="@+id/highAutoNumText"
        app:layout_constraintTop_toTopOf="@+id/highAutoNumText"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/midAutoIncreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/Inc_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintStart_toEndOf="@+id/midAutoNumText"
        app:layout_constraintTop_toTopOf="@+id/midAutoNumText"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/lowAutoIncreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/Inc_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintStart_toEndOf="@+id/lowAutoNumText"
        app:layout_constraintTop_toTopOf="@+id/lowAutoNumText"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/highAutoNumText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/_0_buckets"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/hiAutoText" />

    <TextView
        android:id="@+id/cubeNumText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/_0_cubes"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/divider2"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AutoText" />

    <TextView
        android:id="@+id/coneNumText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="@string/_0_cones"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/divider2"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cubeNumText" />

    <Button
        android:id="@+id/coneIncreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/Inc_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintStart_toEndOf="@+id/coneNumText"
        app:layout_constraintTop_toTopOf="@+id/coneNumText"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/coneDecreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/dec_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintEnd_toStartOf="@+id/coneNumText"
        app:layout_constraintTop_toTopOf="@+id/coneNumText"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/hiAutoDecreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/dec_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintEnd_toStartOf="@+id/highAutoNumText"
        app:layout_constraintTop_toBottomOf="@+id/hiAutoText"
        app:layout_constraintTop_toTopOf="@+id/highAutoNumText"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/cubeDecreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/dec_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintEnd_toStartOf="@+id/cubeNumText"
        app:layout_constraintTop_toTopOf="@+id/cubeNumText"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/lowAutoDecreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/dec_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintEnd_toStartOf="@+id/lowAutoNumText"
        app:layout_constraintTop_toTopOf="@+id/lowAutoNumText"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/midAutoDecreaseBtn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:text="@string/dec_Button"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintEnd_toStartOf="@+id/midAutoNumText"
        app:layout_constraintTop_toBottomOf="@+id/midAutoText"
        app:strokeWidth="1.5dp"
        app:layout_constraintTop_toTopOf="@+id/midAutoNumText"
        tools:ignore="TouchTargetSizeCheck" />

    <LinearLayout
        android:id="@+id/layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:orientation="horizontal"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible"></LinearLayout>

    <View
        android:id="@+id/verticalDivider"
        android:layout_width="2dp"
        android:layout_height="180dp"
        android:layout_marginStart="19dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintStart_toEndOf="@+id/scoutedByHelp"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/horizontalDivider"
        android:layout_width="293dp"
        android:layout_height="3dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/verticalDivider" />

    <View
        android:id="@+id/horizontalDivider3"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="2dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider2" />

    <View
        android:id="@+id/horizontalDivider2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="8dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toTopOf="@+id/comments"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/teamNumber"
        android:layout_width="215dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="#00000000"
        android:entries="@array/teams"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/teamNumberHelp"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="@string/Help"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintStart_toEndOf="@+id/teamNumber"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeWidth="1.5dp"
        tools:ignore="DuplicateSpeakableTextCheck,TouchTargetSizeCheck" />

    <Spinner
        android:id="@+id/matchNumber"
        android:layout_width="@dimen/text_box_width"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:background="#00000000"
        android:entries="@array/matches"
        app:layout_constraintStart_toStartOf="@+id/teamNumber"
        app:layout_constraintTop_toBottomOf="@+id/teamNumber" />

    <Button
        android:id="@+id/matchNumberHelp"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/Help"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintStart_toEndOf="@+id/matchNumber"
        app:layout_constraintTop_toBottomOf="@+id/teamNumberHelp"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/score"
        android:layout_width="130dp"
        android:layout_height="35dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="12dp"
        android:ems="10"
        android:hint="@string/scoreTXT"
        android:importantForAutofill="no"
        android:inputType="number"
        android:minHeight="48dp"
        android:textColor="@color/black"
        android:textSize="14dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/submit"
        app:layout_constraintEnd_toStartOf="@+id/submit"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/comments" />

    <Button
        android:id="@+id/scoreHelp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="7dp"
        android:text="@string/Help"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintBottom_toTopOf="@+id/submit"
        app:layout_constraintStart_toEndOf="@+id/score"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/mobilityHelp"
        android:layout_width="40dp"
        android:layout_height="48dp"
        android:layout_marginTop="3dp"
        android:text="@string/Help"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintStart_toEndOf="@+id/mobilitySelector"
        app:layout_constraintTop_toBottomOf="@id/AutoText"
        app:layout_constraintTop_toTopOf="@+id/mobilitySelector"
        app:strokeWidth="1.5dp"
        tools:ignore="DuplicateSpeakableTextCheck,TouchTargetSizeCheck" />

    <ToggleButton
        android:id="@+id/mobilitySelector"
        android:layout_width="235dp"
        android:layout_height="48dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="168dp"
        android:background="#FFEB3B"
        android:textColor="#000000"
        android:textOff="@string/mobilityOffText"
        android:textOn="@string/mobilityOnText"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/AutoText"
        tools:ignore="TextContrastCheck" />

    <ToggleButton
        android:id="@+id/parkedSelector"
        android:layout_width="235dp"
        android:layout_height="48dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="8dp"
        android:background="#FFEB3B"
        android:textColor="#000000"
        android:textColorHighlight="#FFD600"
        android:textColorLink="#FFD600"
        android:textOff="@string/parkedOffText"
        android:textOn="@string/parkedOnText"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/horizontalDivider2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/telePlatform"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/parkedHelp"
        android:layout_width="40dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:text="@string/Help"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/parkedSelector"
        app:layout_constraintTop_toTopOf="@+id/parkedSelector"
        app:strokeWidth="1.5dp"
        tools:ignore="DuplicateSpeakableTextCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/scoutedBy"
        android:layout_width="@dimen/text_box_width"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="@string/scouterTXT"
        android:importantForAutofill="no"
        android:inputType="text"
        android:minHeight="48dp"
        android:textColor="@color/black"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@+id/matchNumber"
        app:layout_constraintTop_toBottomOf="@+id/matchNumber"
        tools:visibility="visible" />

    <Button
        android:id="@+id/scoutedByHelp"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="8dp"
        android:text="@string/Help"
        android:textColor="@color/buttonTextColor"
        app:layout_constraintStart_toEndOf="@+id/scoutedBy"
        app:layout_constraintTop_toBottomOf="@+id/matchNumberHelp"
        app:strokeWidth="1.5dp"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/comments"
        android:layout_width="400dp"
        android:layout_height="139dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="8dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="@string/comments"
        android:importantForAutofill="no"
        android:inputType="textMultiLine|text"
        android:minHeight="32dp"
        android:textColor="@color/black"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TouchTargetSizeCheck"
        tools:visibility="visible" />

    <Button
        android:id="@+id/submit"
        android:layout_width="174dp"
        android:layout_height="47dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="8dp"
        android:text="@string/submit"
        android:textColor="@color/buttonTextColor"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/comments"
        app:strokeWidth="1.5dp"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/AutoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginEnd="23dp"
        android:text="@string/teleopGroupLabelText"
        android:textAlignment="textEnd"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/divider2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/horizontalDivider" />

    <TextView
        android:id="@+id/autoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginEnd="130dp"
        android:text="@string/autoGroupLabelText"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/divider2"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider2"
        android:layout_width="2dp"
        android:layout_height="315dp"
        android:layout_marginTop="184dp"
        android:background="?android:attr/listDivider"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/hiAutoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="75dp"
        android:text="@string/high_goal"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/autoText" />

    <TextView
        android:id="@+id/lowAutoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="75dp"
        android:text="@string/low_goal"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toEndOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/highAutoNumText" />

    <TextView
        android:id="@+id/midAutoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="75dp"
        android:text="@string/middle_goal"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toEndOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/highAutoNumText" />

    <Spinner
        android:id="@+id/autoPlatform"
        style="@android:style/Widget.Material.Light.Spinner"
        android:layout_width="300dp"
        android:layout_height="33dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="36dp"
        android:entries="@array/endgame"
        app:layout_constraintBottom_toTopOf="@+id/horizontalDivider3"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="DuplicateSpeakableTextCheck,TouchTargetSizeCheck" />

    <Spinner
        android:id="@+id/telePlatform"
        style="@android:style/Widget.Material.Light.Spinner"
        android:layout_width="301dp"
        android:layout_height="58dp"
        android:layout_marginTop="12dp"
        android:entries="@array/endgame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.016"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider2"
        tools:ignore="DuplicateSpeakableTextCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/gameResultText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="6dp"
        android:text="@string/game_result"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/score"
        app:layout_constraintStart_toEndOf="@+id/comments" />

    <Spinner
        android:id="@+id/gameResult"
        style="@android:style/Widget.Material.Light.Spinner"
        android:layout_width="135dp"
        android:layout_height="22dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="6dp"
        android:entries="@array/winLossTie"
        app:layout_constraintBottom_toTopOf="@+id/scoreHelp"
        app:layout_constraintStart_toEndOf="@+id/gameResultText"
        tools:ignore="TouchTargetSizeCheck" />

    <Switch
        android:id="@+id/defenseSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:scrollbarThumbHorizontal="@android:drawable/editbox_background"
        android:textColorHighlight="#FFC107"
        android:textColorLink="#FFEB3B"
        android:textOff="EEOFF"
        android:textOn="EEON"
        app:layout_constraintEnd_toStartOf="@+id/divider2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/defPlaysText" />

    <TextView
        android:id="@+id/defPlaysText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="260dp"
        android:scrollbarThumbHorizontal="@android:drawable/editbox_background"
        android:text="@string/defense_plays"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/divider2"
        app:layout_constraintHorizontal_bias="0.136"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/divider2" />

</androidx.constraintlayout.widget.ConstraintLayout>