                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="@string/defaultValue"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@+id/hiMakeAutoDownBtn"
                app:layout_constraintTop_toTopOf="@+id/hiMakeAutoDownBtn" />

            <Button
                android:id="@+id/highMakeAutoUpBtn"
                android:layout_width="42dp"
                android:layout_height="42dp"
                android:layout_marginStart="16dp"
                android:text="@string/increment"
                app:layout_constraintStart_toEndOf="@+id/hiAutoMakeNumText"
                app:layout_constraintTop_toTopOf="@+id/hiAutoMakeNumText"
