                android:layout_marginTop="8dp"
                android:text="@string/decrement"
                app:layout_constraintStart_toStartOf="@+id/hiMissAutoDownBtn"
                app:layout_constraintTop_toBottomOf="@+id/hiMissAutoDownBtn"
                tools:ignore="TouchTargetSizeCheck" />

            <TextView
